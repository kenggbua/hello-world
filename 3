 public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();
        int z = 1;
        int sum = 0;

        System.out.println(z);
        while (n > 1) {
             z = z + 2;
             sum += z;
            n--;
            System.out.println(z);
        }
        System.out.println(sum + 1);
    }
